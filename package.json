{
  "name": "riff-chunks",
  "version": "7.0.0-alpha.0",
  "description": "Read and write the chunks of RIFF and RIFX files.",
  "homepage": "https://github.com/rochars/riff-chunks",
  "author": "Rafael S. Rocha <rocha.rafaelsilva@gmail.com>",
  "license": "MIT",
  "main": "./dist/riff-chunks.cjs.js",
  "module": "./main.js",
  "browser": "./dist/riff-chunks.umd.js",
  "jsdelivr": "./dist/riff-chunks.min.js",
  "unpkg": "./dist/riff-chunks.min.js",
  "engines": {
    "node": ">=7"
  },
  "keywords": [
    "RIFF",
    "RIFX",
    "LIST",
    "chunk",
    "parser",
    "read",
    "write",
    "file"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/rochars/riff-chunks.git"
  },
  "bugs": {
    "url": "https://github.com/rochars/riff-chunks/issues"
  },
  "scripts": {
    "lint": "jshint main.js",
    "test": "node -r esm ./node_modules/mocha/bin/_mocha test --recursive",
    "test-min": "nyc ./node_modules/mocha/bin/_mocha test --min --recursive",
    "test-cjs": "nyc ./node_modules/mocha/bin/_mocha test --cjs --recursive",
    "test-umd": "nyc ./node_modules/mocha/bin/_mocha test --umd --recursive",
    "pack": "npm test && webpack && npm run test-min && npm run test-cjs && npm run test-umd",
    "doc": "./node_modules/.bin/jsdoc main.js -d docs -t node_modules/docdash -r README.md",
    "build": "npm run lint && npm run pack && npm run doc",
    "coverage": "nyc report --reporter=lcov > coverage.lcov && codecov"
  },
  "devDependencies": {
    "browser-env": "^3.2.4",
    "codecov": "^3.0.0",
    "docdash": "^0.4.0",
    "esm": "^3.0.51",
    "google-closure-compiler-js": "^20180610.0.0",
    "jsdoc": "~3.5.5",
    "jshint": "^2.9.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^3.2.2",
    "webpack": "^3.8.0"
  },
  "dependencies": {
    "byte-data": "^11.0.0-alpha.0"
  }
}
